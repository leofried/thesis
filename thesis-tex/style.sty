    \newcommand{\theo}[4]{
        \begin{theorem}{#1}{#4}
            #2
            \tcblower
            \begin{proof}
            #3
            \end{proof}
      \end{theorem}
    }
    
    \newcommand{\lemm}[4]{
        \begin{lemma}{#1}{#4}
            #2
            \tcblower
            \begin{proof}
            #3
            \end{proof}
       \end{lemma}
    }

    \newcommand{\corr}[4]{
        \begin{corollary}{#1}{#4}
            #2
            \tcblower
            \begin{proof}
            #3
            \end{proof}
       \end{corollary}
    }

