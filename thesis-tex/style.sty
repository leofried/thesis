    \newcommand{\fig}[3]{
        \begin{figg}{#3}{#2}
            \centering
            \graph{#1}{#2}
        \end{figg}
    }

    \newcommand{\phig}[2]{
        \begin{figg}{#2}{#1}
        \end{figg}
    }

    \makeatletter
